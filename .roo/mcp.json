{
  "mcpServers": {
    "filesystem": {
      "command": "npx",
      "args": [
        "-y",
        "@modelcontextprotocol/server-filesystem",
        "${HOME}/finance_feedback_engine-2.0"
      ]
    },
    "redis": {
      "command": "npx",
      "args": [
        "-y",
        "@modelcontextprotocol/server-redis",
        "--host",
        "localhost",
        "--port",
        "6379"
      ]
    },
    "http": {
      "command": "npx",
      "args": [
        "-y",
        "@modelcontextprotocol/server-http"
      ]
    },
    "1mcpserver": {
      "url": "https://mcp.1mcpserver.com/mcp/",
      "headers": {
        "Accept": "text/event-stream",
        "Cache-Control": "no-cache"
      }
    },
    "git": {
      "command": "uvx",
      "args": [
        "mcp-server-git",
        "--repository",
        "~/finance_feedback_engine-2.0"
      ],
      "alwaysAllow": []
    },
    "sequentialthinking": {
      "command": "npx",
      "args": [
        "-y",
        "@modelcontextprotocol/server-sequential-thinking"
      ]
    },
    "memory": {
      "command": "npx",
      "args": [
        "-y",
        "@modelcontextprotocol/server-memory"
      ]
    },
    "codegen": {
      "command": "uvx",
      "args": [
        "--from",
        "git+https://github.com/codegen-sh/codegen-sdk.git#egg=codegen-mcp-server&subdirectory=codegen-examples/examples/codegen-mcp-server",
        "codegen-mcp-server"
      ]
    },
    "composer-trade": {
      "command": "uvx",
      "args": [
        "composer-trade-mcp==0.1.4",
        "--hash",
        "sha256:37d8fcfd6ddc9c8155633f755159f3fcb1a63e7acc2a32513613341a851c84e2"
      ],
      "env": {
        "COMPOSER_API_KEY": "",
        "COMPOSER_SECRET_KEY": ""
      }
    }
  }
}