[run]
source = finance_feedback_engine
omit = 
    */tests/*
    */venv/*
    */.venv/*
    */__pycache__/*
    */site-packages/*
    */migrations/*
    */scripts/*
    */.pytest_cache/*
    */htmlcov/*
    */build/*
    */dist/*
    */.git/*
    */.tox/*

[report]
# Regexes for lines to exclude from consideration
exclude_lines =
    # Have to re-enable the standard pragma
    pragma: no cover

    # Don't complain about missing debug-only code:
    def __repr__
    def __str__

    # Don't complain if tests don't hit defensive assertion code:
    raise AssertionError
    raise NotImplementedError

    # Don't complain if non-runnable code isn't run:
    if __name__ == .__main__.:
    if TYPE_CHECKING:
    if typing.TYPE_CHECKING:

    # Don't complain about abstract methods:
    @abstractmethod
    @abc.abstractmethod

    # Don't complain about deprecated code:
    warnings.warn
    DeprecationWarning

precision = 2
show_missing = True
skip_covered = False

[html]
directory = htmlcov
title = Finance Feedback Engine - Test Coverage Report

[xml]
output = coverage.xml

[json]
output = coverage.json
pretty_print = true
