{
  "dashboard": {
    "title": "FFE Trading Performance Dashboard",
    "tags": ["trading", "ffe", "performance"],
    "timezone": "browser",
    "editable": true,
    "hideControls": false,
    "graphTooltip": 1,
    "panels": [
      {
        "id": 1,
        "title": "Cumulative P&L",
        "type": "graph",
        "gridPos": {"x": 0, "y": 0, "w": 12, "h": 8},
        "targets": [
          {
            "format": "time_series",
            "rawSql": "SELECT exit_time::date as time, SUM(realized_pnl) OVER (ORDER BY exit_time::date) as cumulative_pnl FROM trade_outcomes ORDER BY time",
            "refId": "A"
          }
        ],
        "yaxes": [
          {"format": "currencyUSD", "label": "P&L"},
          {"format": "short"}
        ],
        "lines": true,
        "fill": 2,
        "linewidth": 2
      },
      {
        "id": 2,
        "title": "Daily Trade Count",
        "type": "graph",
        "gridPos": {"x": 12, "y": 0, "w": 12, "h": 8},
        "targets": [
          {
            "format": "time_series",
            "rawSql": "SELECT exit_time::date as time, COUNT(*) as trades FROM trade_outcomes GROUP BY exit_time::date ORDER BY time",
            "refId": "A"
          }
        ],
        "yaxes": [
          {"format": "short", "label": "Trades"},
          {"format": "short"}
        ],
        "bars": true,
        "lines": false
      },
      {
        "id": 3,
        "title": "Win Rate %",
        "type": "graph",
        "gridPos": {"x": 0, "y": 8, "w": 12, "h": 8},
        "targets": [
          {
            "format": "time_series",
            "rawSql": "SELECT exit_time::date as time, ROUND(100.0 * SUM(CASE WHEN realized_pnl > 0 THEN 1 ELSE 0 END) / COUNT(*), 1) as win_rate FROM trade_outcomes GROUP BY exit_time::date ORDER BY time",
            "refId": "A"
          }
        ],
        "yaxes": [
          {"format": "percent", "label": "Win Rate", "min": 0, "max": 100},
          {"format": "short"}
        ],
        "lines": true,
        "fill": 1,
        "thresholds": [
          {"value": 45, "colorMode": "critical", "op": "lt", "fill": true, "line": true}
        ]
      },
      {
        "id": 4,
        "title": "Active Positions",
        "type": "stat",
        "gridPos": {"x": 12, "y": 8, "w": 6, "h": 4},
        "targets": [
          {
            "format": "table",
            "rawSql": "SELECT COUNT(*) as positions FROM active_positions",
            "refId": "A"
          }
        ],
        "options": {
          "graphMode": "none",
          "colorMode": "background",
          "justifyMode": "center",
          "textMode": "value_and_name"
        },
        "fieldConfig": {
          "defaults": {
            "thresholds": {
              "steps": [
                {"value": 0, "color": "green"},
                {"value": 4, "color": "yellow"},
                {"value": 6, "color": "red"}
              ]
            }
          }
        }
      },
      {
        "id": 5,
        "title": "Total Unrealized P&L",
        "type": "stat",
        "gridPos": {"x": 18, "y": 8, "w": 6, "h": 4},
        "targets": [
          {
            "format": "table",
            "rawSql": "SELECT SUM(unrealized_pnl::float) as total_pnl FROM active_positions",
            "refId": "A"
          }
        ],
        "options": {
          "graphMode": "area",
          "colorMode": "value",
          "textMode": "value_and_name"
        },
        "fieldConfig": {
          "defaults": {
            "unit": "currencyUSD",
            "thresholds": {
              "steps": [
                {"value": -100, "color": "red"},
                {"value": 0, "color": "yellow"},
                {"value": 50, "color": "green"}
              ]
            }
          }
        }
      },
      {
        "id": 6,
        "title": "P&L by Asset",
        "type": "bargauge",
        "gridPos": {"x": 0, "y": 16, "w": 24, "h": 8},
        "targets": [
          {
            "format": "table",
            "rawSql": "SELECT product, SUM(realized_pnl) as total_pnl FROM trade_outcomes WHERE exit_time > NOW() - INTERVAL '30 days' GROUP BY product ORDER BY total_pnl DESC LIMIT 10",
            "refId": "A"
          }
        ],
        "options": {
          "displayMode": "gradient",
          "orientation": "horizontal",
          "showUnfilled": true
        },
        "fieldConfig": {
          "defaults": {
            "unit": "currencyUSD",
            "thresholds": {
              "steps": [
                {"value": -1000, "color": "red"},
                {"value": 0, "color": "yellow"},
                {"value": 100, "color": "green"}
              ]
            }
          }
        }
      },
      {
        "id": 7,
        "title": "Profit Factor Trend",
        "type": "graph",
        "gridPos": {"x": 12, "y": 12, "w": 12, "h": 8},
        "targets": [
          {
            "format": "time_series",
            "rawSql": "WITH daily_wins_losses AS (SELECT exit_time::date as time, SUM(CASE WHEN realized_pnl > 0 THEN realized_pnl ELSE 0 END) as wins, SUM(CASE WHEN realized_pnl < 0 THEN ABS(realized_pnl) ELSE 0 END) as losses FROM trade_outcomes GROUP BY exit_time::date) SELECT time, CASE WHEN losses > 0 THEN wins / losses ELSE 0 END as profit_factor FROM daily_wins_losses ORDER BY time",
            "refId": "A"
          }
        ],
        "yaxes": [
          {"format": "short", "label": "Profit Factor"},
          {"format": "short"}
        ],
        "lines": true,
        "fill": 2,
        "thresholds": [
          {"value": 1.2, "colorMode": "critical", "op": "lt", "fill": false, "line": true}
        ]
      },
      {
        "id": 8,
        "title": "Average Trade Duration (Hours)",
        "type": "stat",
        "gridPos": {"x": 0, "y": 12, "w": 6, "h": 4},
        "targets": [
          {
            "format": "table",
            "rawSql": "SELECT AVG(holding_duration_seconds) / 3600.0 as avg_hours FROM trade_outcomes WHERE exit_time > NOW() - INTERVAL '7 days'",
            "refId": "A"
          }
        ],
        "options": {
          "graphMode": "none",
          "colorMode": "background",
          "textMode": "value_and_name"
        },
        "fieldConfig": {
          "defaults": {
            "unit": "h",
            "decimals": 1
          }
        }
      },
      {
        "id": 9,
        "title": "Sharpe Ratio (30d)",
        "type": "stat",
        "gridPos": {"x": 6, "y": 12, "w": 6, "h": 4},
        "targets": [
          {
            "format": "table",
            "rawSql": "WITH daily_pnl AS (SELECT exit_time::date as date, SUM(realized_pnl) as daily_pnl FROM trade_outcomes WHERE exit_time > NOW() - INTERVAL '30 days' GROUP BY exit_time::date) SELECT (AVG(daily_pnl) / NULLIF(STDDEV(daily_pnl), 0)) * SQRT(252) as sharpe FROM daily_pnl",
            "refId": "A"
          }
        ],
        "options": {
          "graphMode": "none",
          "colorMode": "value",
          "textMode": "value_and_name"
        },
        "fieldConfig": {
          "defaults": {
            "decimals": 2,
            "thresholds": {
              "steps": [
                {"value": 0, "color": "red"},
                {"value": 0.5, "color": "yellow"},
                {"value": 1.0, "color": "green"},
                {"value": 2.0, "color": "blue"}
              ]
            }
          }
        }
      }
    ],
    "time": {
      "from": "now-30d",
      "to": "now"
    },
    "timepicker": {
      "refresh_intervals": ["5m", "15m", "30m", "1h", "2h", "1d"]
    },
    "refresh": "5m"
  },
  "overwrite": true
}
